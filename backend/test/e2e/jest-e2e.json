{
	"moduleFileExtensions": [
		"js",
		"json",
		"ts"
	],
	"rootDir": ".",
	"testEnvironment": "node",
	"testRegex": ".e2e-spec.ts$",
	"transform": {
		"^.+\\.(t|j)s$": "ts-jest"
	},
	"moduleNameMapper": {
		"@src/(.*)": "<rootDir>/../../src/$1",
		"@app/(.*)": "<rootDir>/../../src/app/$1",
		"@swapi/(.*)": "<rootDir>/../../src/swapi/$1"
	}
}